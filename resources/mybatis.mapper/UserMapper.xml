<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aviccii.mapper.UserMapper">
    
    <select id="getAllUser" resultType="com.aviccii.pojo.User">
        SELECT * FROM user
    </select>
    
    <select id="loginPage" parameterType="String" resultType="com.aviccii.pojo.User">
        SELECT name FROM user WHERE name=#{0} and id=#{1}
    </select>
</mapper>